{
  "name": "jira-plus-webcomponents",
  "version": "0.0.4",
  "description": "Web components for Jira plus",
  "main": "build/index.cjs.js",
  "module": "dist/index.js",
  "es2015": "build/esm/index.mjs",
  "es2017": "build/esm/index.mjs",
  "types": "dist/index.d.ts",
  "collection": "build/collection/collection-manifest.json",
  "collection:main": "build/collection/index.js",
  "unpkg": "build/jira-plus-webcomponents/jira-plus-webcomponents.esm.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/ionic-team/stencil-component-starter.git"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "stencil build --docs && npm pack .",
    "start": "stencil build --dev --watch --serve",
    "test": "stencil test --spec --e2e",
    "test.watch": "stencil test --spec --e2e --watchAll",
    "generate": "stencil generate"
  },
  "dependencies": {
    "@stencil/core": "^2.17.3"
  },
  "devDependencies": {
    "@stencil/sass": "^1.5.2",
    "@types/jest": "^27.0.3",
    "jest": "^27.4.5",
    "jest-cli": "^27.4.5",
    "puppeteer": "^10.0.0"
  },
  "license": "MIT"
}
