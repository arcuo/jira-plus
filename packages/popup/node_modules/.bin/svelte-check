#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/benjamin/Repos/jira-plus/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="$NODE_PATH:/home/benjamin/Repos/jira-plus/node_modules/.pnpm/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../node_modules/.pnpm/svelte-check@2.10.3_svelte@3.55.1/node_modules/svelte-check/bin/svelte-check" "$@"
else
  exec node  "$basedir/../../../../node_modules/.pnpm/svelte-check@2.10.3_svelte@3.55.1/node_modules/svelte-check/bin/svelte-check" "$@"
fi
